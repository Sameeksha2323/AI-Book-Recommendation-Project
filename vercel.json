{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "app.py"
    },
    {
      "src": "/recommend",
      "dest": "app.py"
    },
    {
      "src": "/recommend_books",  // <-- NEW ROUTE FOR /recommend_books
      "dest": "app.py"         // <--  Direct to app.py to handle it
    }
  ]
}
